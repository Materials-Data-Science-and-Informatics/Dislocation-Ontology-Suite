        [ a owl:Restriction ;
            owl:onProperty <https://purls.helmholtz-metadaten.de/disos/diso#resultsIn> ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/disos/diso#LatticeDisplacement> ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
