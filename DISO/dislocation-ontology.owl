            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="https://purls.helmholtz-metadaten.de/disos/diso#FamilyOfCrystalPlane"/>
        <rdfs:comment xml:lang="en">A set of symmetrically equivalent directions in the lattice.</rdfs:comment>
        <rdfs:comment xml:lang="de">die Menge aller symmetrisch äquivalenten Richtungen.</rdfs:comment>
        <rdfs:label xml:lang="en">Family of Crystal Direction</rdfs:label>
        <rdfs:label xml:lang="de">die Familie der Gitterrichtung</rdfs:label>
    </owl:Class>
    


    <!-- https://purls.helmholtz-metadaten.de/disos/diso#FamilyOfCrystalPlane -->

