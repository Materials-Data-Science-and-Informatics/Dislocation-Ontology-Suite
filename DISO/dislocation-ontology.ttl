            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty <https://purls.helmholtz-metadaten.de/disos/diso#hasSlipDirection> ;
                        owl:someValuesFrom <https://purls.helmholtz-metadaten.de/disos/diso#SlipDirection> ] [ a owl:Restriction ;
                        owl:allValuesFrom <https://purls.helmholtz-metadaten.de/disos/diso#SlipDirection> ;
                        owl:onProperty <https://purls.helmholtz-metadaten.de/disos/diso#hasSlipDirection> ] ) ],
