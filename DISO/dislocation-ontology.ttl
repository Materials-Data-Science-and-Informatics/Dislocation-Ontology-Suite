                        owl:someValuesFrom <https://purls.helmholtz-metadaten.de/disos/diso#BurgersVector> ] [ a owl:Restriction ;
                        owl:allValuesFrom <https://purls.helmholtz-metadaten.de/disos/diso#BurgersVector> ;
