    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/disos/diso#movesOn">
        <rdfs:domain rdf:resource="https://purls.helmholtz-metadaten.de/disos/diso#Dislocation"/>
        <rdfs:range rdf:resource="https://purls.helmholtz-metadaten.de/disos/diso#SlipPlane"/>
        <rdfs:comment xml:lang="en">movesOn represents the relationship between dislocation and slip plane.</rdfs:comment>
        <rdfs:label xml:lang="en">moves on</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/disos/diso#observedBy -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/disos/diso#observedBy">
        <owl:inverseOf rdf:resource="https://purls.helmholtz-metadaten.de/disos/diso#observes"/>
        <rdfs:domain rdf:resource="https://purls.helmholtz-metadaten.de/disos/diso#Dislocation"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://purls.helmholtz-metadaten.de/disos/diso#FieldIonMicroscopy"/>
                    <rdf:Description rdf:about="https://purls.helmholtz-metadaten.de/disos/diso#TransmissionElectronMicroscopy"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">observedBy represents the relationship between dislocation and microscopy techniques, e.g., TEM, FIM, etc.</rdfs:comment>
        <rdfs:label xml:lang="en">observed by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/disos/diso#observes -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/disos/diso#observes">
        <rdfs:comment xml:lang="en">Inverse property of observedBy</rdfs:comment>
        <rdfs:label xml:lang="en">observes</rdfs:label>
    </owl:ObjectProperty>
    


