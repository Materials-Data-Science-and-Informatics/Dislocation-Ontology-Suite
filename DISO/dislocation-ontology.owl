    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/disos/diso#isRepresentationOf -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/disos/diso#isRepresentationOf">
        <rdfs:comment xml:lang="en">Inverse property of hasRepresentation</rdfs:comment>
