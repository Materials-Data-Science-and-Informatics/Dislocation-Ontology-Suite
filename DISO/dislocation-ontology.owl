        <rdfs:range rdf:resource="https://purls.helmholtz-metadaten.de/disos/diso#SlipSystem"/>
        <rdfs:comment xml:lang="en">hasSlipSystem represents the relationship between a crystal structure and slip system.</rdfs:comment>
        <rdfs:label xml:lang="en">has slip system</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/disos/diso#hasVectorOrigin -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/disos/diso#hasVectorOrigin">
