cso:hasBasis a owl:ObjectProperty ;
    rdfs:label "has basis"@en ;
    rdfs:comment "hasBasis represents the relationship between a lattice or coordinate vector basis and basis."@en ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( cso:Lattice cso:VectorComponentOfBasis ) ] ;
    rdfs:range mdo:Basis ;
    owl:inverseOf cso:isBasisOf .

ns1:hasSlipDirection a owl:ObjectProperty ;
    rdfs:label "has slip direction"@en ;
    rdfs:comment "hasSlipDirection represents the relationship between the slip plane or slip system with the slip direction."@en ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( ns1:SlipPlane ns1:SlipSystem ) ] ;
    rdfs:range ns1:SlipDirection ;
    rdfs:subPropertyOf ns1:hasLatticeDirection .

ns1:hasSlipPlaneNormal a owl:ObjectProperty ;
    rdfs:label "has slip plane normal"@en ;
    rdfs:comment "hasSlipPlaneNormal represents the relationship between a slip plane or slip system with slip plane normal."@en ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( ns1:SlipPlane ns1:SlipSystem ) ] ;
    rdfs:range ns1:SlipPlaneNormal ;
    owl:inverseOf ns1:isSlipPlaneNormalOf .

mdo:hasPointGroup a owl:ObjectProperty ;
    rdfs:label "has point group"@en ;
    rdfs:comment "hasPointGroup represents the relationship between a space group and point groups it corresponds to."@en ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( cso:CrystalSystem mdo:SpaceGroup ) ] ;
    rdfs:range mdo:PointGroup .

cso:UnitCell a owl:Class ;
    rdfs:label "Unit Cell"@en ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty cso:hasLatticeParameterLength ;
                        owl:someValuesFrom cso:LatticeParameterLength ] [ a owl:Restriction ;
                        owl:allValuesFrom cso:LatticeParameterLength ;
                        owl:onProperty cso:hasLatticeParameterLength ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty cso:hasLatticeParameterAngle ;
                        owl:someValuesFrom cso:LatticeParameterAngle ] [ a owl:Restriction ;
                        owl:allValuesFrom cso:LatticeParameterAngle ;
                        owl:onProperty cso:hasLatticeParameterAngle ] ) ] ;
    skos:defintion "Basic structural unit or building block of the crystal structure and defines the crystal structure by virtue of its geometry and the atom positions within."@en .

cso:Vector a owl:Class ;
    rdfs:label "Vector"@en ;
    rdfs:comment "(Euclidean) vector is used to represent quantities that both magnitude and direction."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:double ;
            owl:onProperty cso:vectorMagnitude ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty cso:hasVectorComponent ;
                        owl:someValuesFrom cso:VectorComponentOfBasis ] [ a owl:Restriction ;
                        owl:allValuesFrom cso:VectorComponentOfBasis ;
                        owl:onProperty cso:hasVectorComponent ] ) ] ;
    skos:defintion "(Euclidean) vector used to represent quantities that both magnitude and direction."@en .

ns1:hasLatticeDirection a owl:ObjectProperty ;
    rdfs:label "has lattice direction"@en ;
    rdfs:comment "hasLatticeDirection represents the relationship between a lattice or a lattice plane with lattice directions."@en ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( cso:Lattice ns1:LatticePlane ) ] ;
    rdfs:range ns1:LatticeDirection ;
    owl:inverseOf ns1:isLatticeDirectionOf .

ns1:hasLatticePoint a owl:ObjectProperty ;
    rdfs:label "has lattice point"@en ;
    rdfs:comment "hasLatticePoint represents the relationship between a lattice or lattice plane or lattice direction with lattice point."@en ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( cso:Lattice ns1:LatticeDirection ns1:LatticePlane ) ] ;
    rdfs:range ns1:LatticePoint ;
    owl:inverseOf ns1:isLatticePointOf .

chebi:CHEBI_33250 a owl:Class ;
    rdfs:label "Atom"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty ns2:formula ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] ;
    skos:defintion "A chemical entity constituting the smallest component of an element having the chemical properties of the element."@en .

cso:LatticeParameterAngle a owl:Class ;
    rdfs:label "Lattice Parameter Angle"@en ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onDataRange xsd:double ;
                        owl:onProperty cso:latticeParameterAngleAlpha ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] [ a owl:Restriction ;
                        owl:onDataRange xsd:double ;
                        owl:onProperty cso:latticeParameterAngleBeta ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] [ a owl:Restriction ;
                        owl:onDataRange xsd:double ;
                        owl:onProperty cso:latticeParameterAngleGamma ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] ) ] ;
    owl:disjointWith cso:LatticeParameterLength ;
    skos:defintion "The three angles of edges in lattice parameter lengths characterizing the unit cell."@en .

cso:VectorComponentOfBasis a owl:Class ;
    rdfs:label "Vector Component of Basis"@en ;
    rdfs:comment "Vector components/vector coordinates of particular basis. It has relation to a specific basis."@en ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty cso:hasBasis ;
                        owl:someValuesFrom mdo:Basis ] [ a owl:Restriction ;
                        owl:allValuesFrom mdo:Basis ;
                        owl:onProperty cso:hasBasis ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onDataRange xsd:double ;
                        owl:onProperty cso:firstAxisComponent ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] [ a owl:Restriction ;
                        owl:onDataRange xsd:double ;
                        owl:onProperty cso:secondAxisComponent ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] [ a owl:Restriction ;
                        owl:onDataRange xsd:double ;
                        owl:onProperty cso:thirdAxisComponent ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] ) ],
        qudt:Quantity .

ns1:LatticePlane a owl:Class ;
    rdfs:label "die Gitterebene"@de,
        "Lattice Plane"@en ;
    rdfs:comment "Die Gitterebene bildet eine unendlich gestreckte Ebene (gekennzeichnet durch eine Ebenennormale), die die Gitterpunkte so schneidet, dass wiederum eine regelmäßige Anordnung der Gitterpunkte in der Ebene entsteht."@de,
        "The lattice plane forms an infinitely stretched plane (characterized through a plane normal) that cuts through lattice points such that, again, a regular arrangement of lattice points in the plane occurs."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty ns1:planeMillerIndice ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty ns1:hasLatticeDirection ;
                        owl:someValuesFrom ns1:LatticeDirection ] [ a owl:Restriction ;
                        owl:allValuesFrom ns1:LatticeDirection ;
                        owl:onProperty ns1:hasLatticeDirection ] ) ],
        [ a owl:Restriction ;
            owl:allValuesFrom ns1:VectorOrigin ;
            owl:onProperty ns1:hasVectorOrigin ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty ns1:hasLatticePoint ;
                        owl:someValuesFrom ns1:LatticePoint ] [ a owl:Restriction ;
                        owl:allValuesFrom ns1:LatticePoint ;
                        owl:onProperty ns1:hasLatticePoint ] ) ],
        ns1:FamilyOfCrystalPlane .

ns1:LatticePoint a owl:Class ;
    rdfs:label "der Gitterpunkt"@de,
        "Lattice Point"@en ;
    rdfs:comment "Der Gitterpunkt ist der Punkt, an dem sich ein Atom oder ein Molekül befindet."@de,
        "Lattice point is the point where atom(s) or molecule(s) is located."@en ;
    rdfs:subClassOf cso:Point .

cso:LatticeParameterLength a owl:Class ;
    rdfs:label "Lattice Parameter Length"@en ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onDataRange xsd:double ;
                        owl:onProperty cso:latticeParameterLengthA ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] [ a owl:Restriction ;
                        owl:onDataRange xsd:double ;
                        owl:onProperty cso:latticeParameterLengthB ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] [ a owl:Restriction ;
                        owl:onDataRange xsd:double ;
                        owl:onProperty cso:latticeParameterLengthC ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] ) ] ;
    skos:defintion "The three edges characterize the unit cell that parallelepiped."@en .

ns1:Dislocation a owl:Class ;
    rdfs:label "die Versetzung"@de,
        "Dislocation"@en ;
    rdfs:comment "Ein linearer oder eindimensionaler Defekt, um den herum einige der Atome falsch ausgerichtet sind. Im Mesoscale ist eine Versetzung ein linienförmiges Objekt, das eine Grenze darstellt, die die Regionen auf der Gleitebene, in denen ein Gleiten stattgefunden hat, von denen trennt, in denen dies nicht der Fall war."@de,
        "Linear or one-dimensional defect around which some of the atoms are misaligned. In the mesoscale, a dislocation is a line object that is a boundary separating the regions on the slip plane which have undergone slip from those that have not."@en,
        "The presence of dislocation introduces local disturbance of the atomic-level geometry."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty ns1:movesOn ;
            owl:someValuesFrom ns1:SlipPlane ],
        [ a owl:Restriction ;
            owl:onProperty ns1:observedBy ;
            owl:someValuesFrom ns1:FieldIonMicroscopy ],
        [ a owl:Restriction ;
            owl:onProperty ns1:observedBy ;
            owl:someValuesFrom ns1:TransmissionElectronMicroscopy ],
        [ a owl:Restriction ;
            owl:onProperty ns1:resultsIn ;
            owl:someValuesFrom ns1:LatticeDisplacement ],
        [ a owl:Restriction ;
            owl:onProperty ns1:hasMathematicalRepresentation ;
            owl:someValuesFrom ns1:Line ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty ns1:hasBurgersVector ;
                        owl:someValuesFrom ns1:BurgersVector ] [ a owl:Restriction ;
                        owl:allValuesFrom ns1:BurgersVector ;
                        owl:onProperty ns1:hasBurgersVector ] ) ],
        ns1:LineDefect .

mdo:Basis a owl:Class ;
    rdfs:label "Basis"@en ;
    rdfs:comment "A basis defines a spatial unit used to express fractional coordinates."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass mdo:CoordinateVector ;
            owl:onProperty cso:hasSecondAxisVector ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass mdo:CoordinateVector ;
            owl:onProperty cso:hasFirstAxisVector ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onClass mdo:CoordinateVector ;
            owl:onProperty cso:hasThirdAxisVector ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] ;
    skos:defintion "Set of vector(in 2-D two vectors and in 3-D three vectors) that linearly independent."@en .

mdo:PointGroup a owl:Class ;
    rdfs:label "Point Group"@en ;
    rdfs:comment "A group of linear mappings of vector space, which is corresonpond to the group of motions in point space determining the symmetry of the macroscopic crystal, is called the point group of the crystal, furthermore of the crystal structure, and is also called the point group of its space group."@en,
        "In the symmetry point of view, a set of point groups (32 point groups) is classified into the crystal system. Note that the point group describes the symmetry of macroscopic or finite object, thus the Bravais lattice (translation symmetry) is negligible."@en ;
    rdfs:seeAlso "International Tables for Crystallography, Volume A, Fourth, revised edition, Section 8.1.5."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty mdo:PointGroupHMName ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onProperty cso:isPointGroupOf ;
            owl:someValuesFrom mdo:SpaceGroup ],
        [ a owl:Restriction ;
            owl:onProperty cso:isPointGroupOf ;
            owl:someValuesFrom cso:CrystalSystem ] .

cso:Lattice a owl:Class ;
    rdfs:label "Lattice"@en ;
    rdfs:comment "Das mathematische Konzept zur Darstellung der Periodizität eines Kristalls. Ein Gitter definiert eine periodische Anordnung von einem oder mehreren Atomen."@de,
        "The mathematical concept to represent the periodicity of a crystal. A lattice defines a periodic arrangement of one or more atoms."@en ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty ns1:hasLatticePoint ;
                        owl:someValuesFrom ns1:LatticePoint ] [ a owl:Restriction ;
                        owl:allValuesFrom ns1:LatticePoint ;
                        owl:onProperty ns1:hasLatticePoint ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty ns1:hasLatticePlane ;
                        owl:someValuesFrom ns1:LatticePlane ] [ a owl:Restriction ;
                        owl:allValuesFrom ns1:LatticeDirection ;
                        owl:onProperty ns1:hasLatticeDirection ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty cso:hasBasis ;
                        owl:someValuesFrom mdo:Basis ] [ a owl:Restriction ;
                        owl:allValuesFrom mdo:Basis ;
                        owl:onProperty cso:hasBasis ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty ns1:hasLatticeDirection ;
                        owl:someValuesFrom ns1:LatticeDirection ] [ a owl:Restriction ;
                        owl:allValuesFrom ns1:LatticeDirection ;
                        owl:onProperty ns1:hasLatticeDirection ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty cso:hasUnitCell ;
                        owl:someValuesFrom cso:UnitCell ] [ a owl:Restriction ;
                        owl:allValuesFrom cso:UnitCell ;
                        owl:onProperty cso:hasUnitCell ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty cso:hasCrystalSystem ;
                        owl:someValuesFrom cso:CrystalSystem ] [ a owl:Restriction ;
                        owl:allValuesFrom cso:CrystalSystem ;
                        owl:onProperty cso:hasCrystalSystem ] ) ] ;
    skos:defintion "A lattice defines a periodic arrangement of one or more atoms."@en .

ns1:LatticeDirection a owl:Class ;
    rdfs:label "Die Gitterrichtung"@de,
        "Lattice Direction"@en ;
    rdfs:comment "Die Vektorrichtung innerhalb des Gitters, die zwei Gitterpunkte verbindet."@de,
        "The vector direction inside the lattice that is connecting two lattice points."@en ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty ns1:hasLatticePoint ;
                        owl:someValuesFrom ns1:LatticePoint ] [ a owl:Restriction ;
                        owl:allValuesFrom ns1:LatticePoint ;
                        owl:onProperty ns1:hasLatticePoint ] ) ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty ns1:directionMillerIndice ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        cso:Vector,
        ns1:FamilyOfCrystalDirection .

mdo:Occupancy a owl:Class ;
    rdfs:label "Occupancy"@en ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty mdo:hasSpecies ;
                        owl:someValuesFrom mdo:Species ] [ a owl:Restriction ;
                        owl:allValuesFrom mdo:Species ;
                        owl:onProperty mdo:hasSpecies ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty mdo:hasSite ;
                        owl:someValuesFrom mdo:Site ] [ a owl:Restriction ;
                        owl:allValuesFrom mdo:Site ;
                        owl:onProperty mdo:hasSite ] ) ] ;
    skos:defintion "An occupancy ties a specific species to a site."@en .

mdo:Site a owl:Class ;
    rdfs:label "Site"@en ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom mdo:CoordinateVector ;
                        owl:onProperty mdo:hasFractionalCoordinates ] [ a owl:Restriction ;
                        owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass mdo:CoordinateVector ;
                        owl:onProperty mdo:hasFractionalCoordinates ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom mdo:CoordinateVector ;
                        owl:onProperty mdo:hasCartesianCoordinates ] [ a owl:Restriction ;
                        owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass mdo:CoordinateVector ;
                        owl:onProperty mdo:hasCartesianCoordinates ] ) ] ;
    skos:defintion "A site is a point in a lattice."@en .

mdo:Species a owl:Class ;
    rdfs:label "Species"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass chebi:CHEBI_33250 ;
            owl:onProperty mdo:hasElement ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] ;
    skos:defintion "A species is the combination of atoms on each site."@en .

cso:CrystalStructure a owl:Class ;
    rdfs:label "Crystal Structure"@en ;
    rdfs:comment "A crystal structure is described by the lattice geometry and atomic arrangements within the unit cell."@en ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom mdo:SpaceGroup ;
                        owl:onProperty mdo:hasSpaceGroup ] [ a owl:Restriction ;
                        owl:onClass mdo:SpaceGroup ;
                        owl:onProperty mdo:hasSpaceGroup ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] ) ],
        [ a owl:Restriction ;
            owl:allValuesFrom cso:Lattice ;
            owl:onProperty cso:hasLattice ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty mdo:hasOccupancy ;
                        owl:someValuesFrom mdo:Occupancy ] [ a owl:Restriction ;
                        owl:allValuesFrom mdo:Occupancy ;
                        owl:onProperty mdo:hasOccupancy ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty ns1:hasSlipSystem ;
                        owl:someValuesFrom ns1:SlipSystem ] [ a owl:Restriction ;
                        owl:allValuesFrom ns1:SlipSystem ;
                        owl:onProperty ns1:hasSlipSystem ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty ns1:hasSlipPlane ;
                        owl:someValuesFrom ns1:SlipPlane ] [ a owl:Restriction ;
                        owl:allValuesFrom ns1:SlipPlane ;
                        owl:onProperty ns1:hasSlipPlane ] ) ] ;
    skos:defintion "A crystal structure is described by both of the lattice geometry and atomic arrangements within the unit cell."@en .

ns1:SlipPlaneNormal a owl:Class ;
    rdfs:label "der Gleitebene Normalvektor"@de,
        "Slip Plane Normal"@en ;
    rdfs:comment "Der Einheitsnormalvektor der Gleitebenen."@de,
        "The unit normal vector of slip planes."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty ns1:directionMillerIndice ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        cso:Vector .

mdo:SpaceGroup a owl:Class ;
    rdfs:label "Space Group"@en ;
    rdfs:comment "A space group is the symmetry group of a three-dimensional crystal pattern is called its space group."@en,
        "Combination of Bravais lattice, point groups, two symmetry operations (screw rotation and glide reflection) results in space groups"@en,
        """The nomenclature consists of a maximum of four symbols for each space group, e.g., Fm-3m(Cubic)
First symbol defines the Bravais lattice centering, here is the cubic centering, C. The next three symbols defines symmetry elements with respect to viewing directions, which are different from crystal system to crystal system."""@en ;
    rdfs:seeAlso "International Tables for Crystallography, Volume !, Fourth, revised edition, Section 8.1.5."@en ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty mdo:hasPointGroup ;
                        owl:someValuesFrom mdo:PointGroup ] [ a owl:Restriction ;
                        owl:allValuesFrom mdo:PointGroup ;
                        owl:onProperty mdo:hasPointGroup ] ) ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty mdo:SpaceGroupSymbol ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:integer ;
            owl:onProperty mdo:SpaceGroupID ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onProperty cso:isSpaceGroupOf ;
            owl:someValuesFrom cso:CrystalStructure ] .

ns1:SlipPlane a owl:Class ;
    rdfs:label "die Gleitebene"@de,
        "Slip Plane"@en ;
    rdfs:comment "Die kristallografische/Gitterebene, entlang derer die Versetzungslinie verläuft/bewegt wird. Die Gleitebene ist in der Regel die Ebene mit der höchsten Dichte an Atomen, d. h. mit dem größten Abstand."@de,
        "The crystallographic/lattice plane along which the dislocation line traverses/moves. The slip plane is usually the plane with the highest density of atoms, i.e. most widely spaced."@en ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty ns1:hasSlipDirection ;
                        owl:someValuesFrom ns1:SlipDirection ] [ a owl:Restriction ;
                        owl:allValuesFrom ns1:SlipDirection ;
                        owl:onProperty ns1:hasSlipDirection ] ) ],
        [ a owl:Restriction ;
            owl:onProperty ns1:activeSlipPlane ;
            owl:someValuesFrom xsd:boolean ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty ns1:hasSlipPlaneNormal ;
                        owl:someValuesFrom ns1:SlipPlaneNormal ] [ a owl:Restriction ;
                        owl:allValuesFrom ns1:SlipPlaneNormal ;
                        owl:onProperty ns1:hasSlipPlaneNormal ] ) ],
        ns1:LatticePlane .

cso:CrystalSystem a owl:Class ;
    rdfs:label "Crystal System"@en ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom mdo:PointGroup ;
                        owl:onProperty mdo:hasPointGroup ] [ a owl:Restriction ;
                        owl:onClass mdo:PointGroup ;
                        owl:onProperty mdo:hasPointGroup ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] ) ] ;
    skos:defintion "A classification scheme for crystal structures on the basis of unit cell geometry."@en .

ns1:SlipDirection a owl:Class ;
    rdfs:label "die Gleitrichtung"@de,
        "Slip Direction"@en ;
    rdfs:comment "Die Richtung in der Gleitebene, in der die plastische Verformung stattfindet. Die Gleitrichtung entspricht einem der kürzesten Translationsvektoren des Gitters."@de,
        "The direction in the slip plane along which plastic deformation takes place. The slip direction corresponds to one of the shortest lattice translation vectors."@en ;
    rdfs:subClassOf ns1:LatticeDirection .

ns1:SlipSystem a owl:Class ;
    rdfs:label "das Gleitsystem"@de,
        "Slip System"@en ;
    rdfs:comment "Ein Gleitsystem ist definiert als die Menge der Gleitebenen mit dem gleichen Einheitsnormalvektor und der gleichen Gleitrichtung."@de,
        "A slip system is defined as the set of slip planes with the same unit normal vector and the same slip direction."@en ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty ns1:hasSlipPlaneNormal ;
                        owl:someValuesFrom ns1:SlipPlaneNormal ] [ a owl:Restriction ;
                        owl:allValuesFrom ns1:SlipPlaneNormal ;
                        owl:onProperty ns1:hasSlipPlaneNormal ] ) ],
        [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty ns1:hasSlipDirection ;
                        owl:someValuesFrom ns1:SlipDirection ] [ a owl:Restriction ;
                        owl:allValuesFrom ns1:SlipDirection ;
                        owl:onProperty ns1:hasSlipDirection ] ) ] .

mdo:CoordinateVector a owl:Class ;
    rdfs:label "Coordinate Vector"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:double ;
            owl:onProperty mdo:Y_axisCoordinate ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:double ;
            owl:onProperty mdo:X_axisCoordinate ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onDataRange xsd:double ;
            owl:onProperty mdo:Z_axisCoordinate ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] ;
