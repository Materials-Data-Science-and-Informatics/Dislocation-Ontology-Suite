        <rdfs:comment xml:lang="en">Inverse property of resultsIn</rdfs:comment>
        <rdfs:label xml:lang="en">is resulted by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/disos/diso#isSlipPlaneNormalOf -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/disos/diso#isSlipPlaneNormalOf">
