    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/disos/diso#isLineSenseOf -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/disos/diso#isLineSenseOf">
        <rdfs:comment xml:lang="en">Inverse property of hasLineSense</rdfs:comment>
        <rdfs:label xml:lang="en">is line sense of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/disos/diso#isMathematicalRepresentationOf -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/disos/diso#isMathematicalRepresentationOf">
