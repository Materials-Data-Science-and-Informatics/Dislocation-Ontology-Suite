        <rdfs:range rdf:resource="https://purls.helmholtz-metadaten.de/disos/diso#VectorOrigin"/>
        <rdfs:comment xml:lang="en">hasVectorOrigin represents the relationship between lattice plane and origin.</rdfs:comment>
        <rdfs:label xml:lang="en">has vector origin</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/disos/diso#isBurgersVectorOf -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/disos/diso#isBurgersVectorOf">
