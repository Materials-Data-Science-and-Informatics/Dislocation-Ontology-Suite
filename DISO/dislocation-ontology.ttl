                        owl:someValuesFrom <http://emmo.info/domain-crystallography/crystallography#EMMO_6a340d67_c6cb_4348_8537_d9aa0e768721> ] [ a owl:Restriction ;
                        owl:allValuesFrom <http://emmo.info/domain-crystallography/crystallography#EMMO_6a340d67_c6cb_4348_8537_d9aa0e768721> ;
                        owl:onProperty <https://purls.helmholtz-metadaten.de/disos/cdo#hasCrystalStructure> ] ) ],
        [ a owl:Class ;
